{
  "name": "jambagecom/taxajax",
  "type": "typo3-cms-extension",
  "description": "Enhancement to the xajax extension in version 0.2.4 with TYPO3 specific code.",
  "keywords": [
    "TYPO3",
    "extension",
    "library",
    "xajax"
  ],
  "support": {
    "issues": "https://github.com/franzholz/taxajax/issues"
  },
  "homepage": "https://ttproducts.de",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://jambage.com"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^12.0 || ^13.0",
    "jambagecom/div2007": "^2.0"
  },
  "autoload": {
    "classmap": [
      "."
    ]
  },
  "replace": {
    "typo3-ter/taxajax": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "taxajax"
    }
  },
  "require-dev": {
    "rector/rector": "^0.18.13"
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
