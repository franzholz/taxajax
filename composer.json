{
  "name": "jambagecom/taxajax",
  "type": "typo3-cms-extension",
  "description": "Enhancement to the xajax extension with TYPO3 specific code.",
  "keywords": [
    "TYPO3",
    "extension",
    "library",
    "xajax"
  ],
  "support": {
    "issues": "https://github.com/franzholz/taxajax/issues"
  },
  "homepage": "https://ttproducts.de",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://jambage.com"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "typo3/cms-core": "^10.4 || ^11.5 || ^12.0",
    "jambagecom/div2007": "^1.18 || ^2.0"
  },
  "autoload": {
    "classmap": [
      "/"
    ]
  },
  "replace": {
    "typo3-ter/taxajax": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "taxajax"
    }
  }
}

